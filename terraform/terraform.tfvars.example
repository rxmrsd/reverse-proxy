# GCP Project Configuration
project_id = "your-gcp-project-id"
region     = "asia-northeast1"

# Artifact Registry
repository_name = "reverse-proxy"

# Cloud Run Service Names
backend_service_name  = "reverse-proxy-backend"
frontend_service_name = "reverse-proxy-frontend"

# Docker Images (optional - if not provided, will use images from Artifact Registry)
# Leave empty to use default: {region}-docker.pkg.dev/{project_id}/{repository_name}/reverse-proxy-{backend|frontend}:{image_tag}
backend_image  = ""
frontend_image = ""

# Image tag to deploy (default: latest)
image_tag = "latest"
